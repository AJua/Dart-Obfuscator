{
  "name": "dart-obfuscator",
  "displayName": "Dart Obfuscator", 
  "description": "Obfuscates Dart code by renaming symbols with random names",
  "version": "1.0.0",
  "publisher": "linchienhung",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AJua/Dart-Obfuscator"
  },
  "keywords": [
    "dart",
    "flutter",
    "obfuscation",
    "code-protection"
  ],
  "activationEvents": [
    "onLanguage:dart"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dart-obfuscator.obfuscateCode",
        "title": "Obfuscate Dart Code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
