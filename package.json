{
  "name": "dart-symbol-printer",
  "displayName": "Dart Symbol Printer",
  "description": "Prints all symbols in a Dart project",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:dart"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dart-symbol-printer.printSymbols",
        "title": "Print Dart Symbols"
      },
      {
        "command": "dart-symbol-printer.refactorSymbols",
        "title": "Refactor All Dart Symbols (Add prefix_)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}