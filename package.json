{
  "name": "dart-obfuscator",
  "displayName": "Code Obfuscator", 
  "description": "Obfuscates code by renaming symbols with random names. Supports Dart/Flutter, C#, Python, and TypeScript/JavaScript. REQUIRES appropriate language extensions to be installed (C# Dev Kit, Python, Dart/Flutter extensions).",
  "version": "1.0.0",
  "publisher": "linchienhung",
  "engines": {
    "vscode": "^1.74.0"
  },
  "extensionDependencies": [],
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AJua/Dart-Obfuscator"
  },
  "keywords": [
    "dart",
    "flutter",
    "csharp",
    "python",
    "typescript",
    "javascript",
    "obfuscation",
    "code-protection"
  ],
  "activationEvents": [
    "onLanguage:dart",
    "onLanguage:csharp",
    "onLanguage:python",
    "onLanguage:typescript",
    "onLanguage:javascript"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dart-obfuscator.obfuscateCode",
        "title": "Obfuscate Code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
